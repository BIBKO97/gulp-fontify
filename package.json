{
  "name": "gulp-fontify",
  "version": "0.0.6",
  "description": "Encode font files as CSS by using Gulp (gulp-fontify).",
  "main": "src/index.js",
  "type": "module",
  "deprecated": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BIBKO97/npm-gulp-fontify.git"
  },
  "dependencies": {
    "css-font-style-keywords": "^1.0.1",
    "css-font-weight-keywords": "^1.0.0",
    "css-font-weight-names": "^0.2.1",
    "gulp-util": "^3.0.7",
    "mime": "^1.3.4",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.9.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "css": "^2.2.1",
    "eslint": "^1.10.3"
  },
  "scripts": {
    "build": "cd src; babel *.js -d ../src",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run build",
    "pretest": "eslint src/**/*.js",
    "test": "ava --require babel-core/register"
  },
  "keywords": [
    "base64",
    "css",
    "font",
    "font-face",
    "gulp",
    "gulp-fontify",
    "otf",
    "ttf",
    "woff",
    "woff2"
  ],
  "author": {
    "name": "BIBKO97",
    "email": "bibko.content@gmail.com"
  },
  "license": "MIT"
